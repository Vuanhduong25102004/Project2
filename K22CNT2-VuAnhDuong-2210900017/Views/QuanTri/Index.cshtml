        <td>
            @Html.DisplayFor(modelItem => item.Mat_khau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trang_thai)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Tai_khoan }, new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Details", "Details", new { id = item.Tai_khoan }, new { @class = "btn btn-success" }) |
