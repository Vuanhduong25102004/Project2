}

<h2>Danh sách tài khoản Admin</h2>


<table class="table">
    <tr>
        <th>
            <span>Tài khoản</span>
        </th>
        <th>
            <span>Mật khẩu</span>
        </th>
        <th>
            <span>Trạng thái</span>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tai_khoan)
